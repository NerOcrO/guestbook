{
  "name": "guestbook",
  "version": "1.0.0",
  "description": "A simple guestbook with many BDD",
  "main": "src/index.js",
  "keywords": [],
  "author": "NerOcrO",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.8.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NerOcrO/guestbook"
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-preset-minify": "^0.5.0",
    "body-parser": "^1.18.3",
    "clean-css-cli": "^4.2.1",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.9.0",
    "date-fns": "^1.30.1",
    "debug": "^4.1.1",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "helmet": "^3.15.1",
    "inquirer": "^6.2.2",
    "knex": "^0.17.6",
    "mysql": "^2.16.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "nodemon": "^1.18.10"
  },
  "scripts": {
    "build_js": "babel src/javascripts --out-file public/script.js --source-maps",
    "build_css": "cleancss -o public/style.css src/css/* --source-map",
    "build": "rm -rf public && mkdir public && npm run build_js && npm run build_css",
    "watch_js": "npm run build_js -- -w",
    "dev": "npm run build && DEBUG=gb nodemon src/index.js",
    "start": "npm run build && node -r @babel/register src/index.js",
    "install": "node -r @babel/register src/config/install-database.js"
  }
}
